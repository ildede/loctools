{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "loctools",
  "compatibility_date": "2025-06-07",
  "build": {
    "command": "pnpm run build"
  },
  "assets": {
    "directory": "./dist/",
    "not_found_handling": "single-page-application"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "loctools.wtf",
      "custom_domain": true
    }
  ]
}
